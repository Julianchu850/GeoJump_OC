    /**
    * Variante del cubo 1 - Patrón geométrico interior
    * Tamaño: 16×16 píxeles (1 palabra de ancho, 16 filas de alto)
    * Diseño: Cuadrado hueco con formas geométricas anidadas en su interior
     */
    function void drawCube1(int location) {
        var int memAddress;
        let memAddress = 16384 + location;
        
        do Memory.poke(memAddress + 0,   -1);      // Fila 0:  ████████████████
        do Memory.poke(memAddress + 32,  -32767);  // Fila 1:  █______________█
        do Memory.poke(memAddress + 64,  -31745);  // Fila 2:  █__██████████__█
        do Memory.poke(memAddress + 96,  -32257);  // Fila 3:  █__█________█__█
        do Memory.poke(memAddress + 128, -32017);  // Fila 4:  █__█_█████__█__█
        do Memory.poke(memAddress + 160, -32145);  // Fila 5:  █__█_█___█__█__█
        do Memory.poke(memAddress + 192, -32145);  // Fila 6:  █__█_█___█__█__█
        do Memory.poke(memAddress + 224, -32017);  // Fila 7:  █__█_█████__█__█
        do Memory.poke(memAddress + 256, -32257);  // Fila 8:  █__█________█__█
        do Memory.poke(memAddress + 288, -31745);  // Fila 9:  █__██████████__█
        do Memory.poke(memAddress + 320, -32767);  // Fila 10: █______________█
        do Memory.poke(memAddress + 352, -31745);  // Fila 11: █__██████████__█
        do Memory.poke(memAddress + 384, -32257);  // Fila 12: █__█________█__█
        do Memory.poke(memAddress + 416, -32289);  // Fila 13: █___█████████__█
        do Memory.poke(memAddress + 448, -32767);  // Fila 14: █______________█
        do Memory.poke(memAddress + 480, -1);      // Fila 15: ████████████████
        
        return;
    }
    
